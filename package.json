{
  "name": "protractor-http-client",
  "version": "1.0.2",
  "description": "HTTP client to be used in protractor tests",
  "keywords": [
    "protractor",
    "http",
    "rest",
    "client",
    "test",
    "selenium",
    "webdriver"
  ],
  "homepage": "https://github.com/marcodave/protractor-http-client",
  "scripts": {
    "build": "npm test",
    "compile": "tsc",
    "pretest": "tsc",
    "test": "webdriver-manager update && protractor dist/test/protractor.conf.js",
    "prepublish": "npm test",
    "postpublish": "npm version patch && git push && git push --tags"
  },
  "dependencies": {
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/request": "2.47.0",
    "@types/jasminewd2": "2.x",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "^4",
    "typescript": "^2.7"
  },
  "author": "Marco Dalla Vecchia <mdallav@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcodave/protractor-http-client.git"
  },
  "main": "dist/lib/http-client.js"
}
